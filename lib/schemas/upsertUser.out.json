{
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "email",
    "mob",
    "address",
    "_id"
  ],
  "properties": {
    "name": {
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Test1"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "xpc@gmail.com"
      ],
      "pattern": "^(.*)$"
    },
    "mob": {
      "type": "string",
      "title": "The Mob Schema",
      "default": "",
      "examples": [
        "908765432"
      ],
      "pattern": "^(.*)$"
    },
    "address": {
      "type": "string",
      "title": "The Address Schema",
      "default": "",
      "examples": [
        "Noida"
      ],
      "pattern": "^(.*)$"
    }
  }
}